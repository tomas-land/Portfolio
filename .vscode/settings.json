{
    "liveSassCompile.settings.formats":[
       {
           "format": "expanded",
           "extensionName": ".css",
           "savePath": "/assets/css"
       },
       {
           "extensionName": ".min.css",
           "format": "compressed",
           "savePath": "/assets/dist/css"
       }
   ],
   "liveSassCompile.settings.excludeList": [
      "**/node_modules/**",
      ".vscode/**"
   ],
   "liveSassCompile.settings.generateMap": true,
   "liveSassCompile.settings.autoprefix": [
       "> 1%",
       "last 6 versions"
],
"compile-hero.disable-compile-files-on-did-save-code": true,
"liveServer.settings.port": 5503,
"workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2c384b",
    "activityBar.activeBorder": "#8d5369",
    "activityBar.background": "#2c384b",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#8d5369",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#2c384b",
    "statusBar.background": "#19202b",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2c384b",
    "statusBarItem.remoteBackground": "#19202b",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#19202b",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#19202b99",
    "titleBar.inactiveForeground": "#e7e7e799"
}
}